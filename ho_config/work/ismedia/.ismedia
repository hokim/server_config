export ismedia_script_path=~/ho_config/work/ismedia
PATH=$ismedia_script_path:$PATH

# ISMEDIA #
alias ismedia-script='cd $ismedia_script_path'

# For The ISMEDIA Project
# Standard JIG
source $ismedia_script_path/standard-jig/.standard-jig
